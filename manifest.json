{
  "manifest_version": 3,
  "name": "Devportal",
  "version": "1.0",
  "description": "Открывает заданный URL в новой вкладке.",
  "permissions": [
    "storage",
    "activeTab",
    "contextMenus"
  ],
  "background": {
    "service_worker": "background.js"
  },
  "action": {
    "default_title": "test",
    "default_icon": {
      "16": "/images/icons8-dev-16.png",
      "32": "/images/icons8-dev-32.png",
      "48": "/images/icons8-dev-48.png",
      "128": "/images/icons8-dev-128.png"
    }
  },
  "icons": {
    "16": "/images/icons8-dev-16.png",
    "32": "/images/icons8-dev-32.png",
    "48": "/images/icons8-dev-48.png",
    "128": "/images/icons8-dev-128.png"
  },
  "options_page": "options.html"
}